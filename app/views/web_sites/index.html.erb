<p id="notice"><%= notice %></p>

<h1>Web Sites</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_sites.each do |web_site| %>
      <tr>
        <td><%= web_site.content %></td>
        <td><%= link_to 'Show', web_site %></td>
        <td><%= link_to 'Edit', edit_web_site_path(web_site) %></td>
        <td><%= link_to 'Destroy', web_site, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web Site', new_web_site_path %>
